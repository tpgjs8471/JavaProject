package day18;

import java.text.SimpleDateFormat;
import java.util.Date;

public class DateFormat01 {

	public static void main(String[] args) {
		/* 날짜의 형식을 맞춰주기 위한 설정
		 * format 설정
		 * 날짜는 년월일(요일)시분초
		 * 날짜를 -> 문자로 변환
		 */
		
		Date date=new Date();
		SimpleDateFormat sdf= new SimpleDateFormat("yyyy-MM-dd (E) hh:mm:ss");
	}

}
