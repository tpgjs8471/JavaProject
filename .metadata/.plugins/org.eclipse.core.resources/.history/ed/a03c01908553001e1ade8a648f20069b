package Aprject;

import java.util.ArrayList;

public class Game {

	public static void main(String[] args) {
		ArrayList<CardDeck> pack = new ArrayList<>();
		ArrayList<CardDeck> User=new ArrayList<>();
		ArrayList<CardDeck> Computer=new ArrayList<>();
		
		int size=pack.size();
		for(int i=0;i<size;i++) {
			if(i>size/2) {
				User.add(pack.get(i));
			}else {
				Computer.add(pack.get(i));
			}
		}
		System.out.println(pack);

	}

}
