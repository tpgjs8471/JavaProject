package day16;

import java.util.ArrayList;
import java.util.List;

public class List01 {

	public static void main(String[] args) {
		/*
		 * 하루 일과를 저장하는 list를 생성 <String>
		 */
		List<String> list=new ArrayList<String>();
		
		list.add("아침");
		list.add("점심");
		list.add("저녁");
		
		
		
		//향상된 for문으로 출력
		for(String tmp:list) {
			System.out.println(tmp+" ");
		}
		
		//Iterator로 출력

	}

}
